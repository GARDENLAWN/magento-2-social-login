    "require": {
        "mageplaza/module-core": "^1.5.13",
        "hybridauth/hybridauth": "^3.8.2 || ^3.11.0",
        "hyva-themes/magento2-theme-module": "^1.3.11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mageplaza",
